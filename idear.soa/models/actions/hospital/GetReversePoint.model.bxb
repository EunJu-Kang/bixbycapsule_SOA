action (GetReversePoint) {
  type(Fetch)
  collect {
    computed-input (point){
      type (geo.GeocodedAddress)
      min (Optional) max (One)
      compute{
        intent{
          goal: geo.GeocodedAddress
          route: geo.CurrentLocation
        }         
      }
    }
  }
  output (geo.GeocodedAddress){
    evaluate{
      $expr(point)
    }
  }
}
    