action (PregnancyStatusAction) {

  description (임신중일 때의 정보 액션)
  collect {
    input (weekNumOperand) {
      type (WeekNumOperand)
      min (Optional) max (One)
    }
    input (weekOperand) {
      type (WeekOperand)
      min (Optional) max (One)
    }
    input (targetOperand) {
      type (TargetOperand)
      min (Required) max (One)
    }
    input (statusOperand) {
      type (StatusOperand)
      min (Required) max (One)
    }

  }
  type(Search)
  output (PregnancyStatusResults)
}
