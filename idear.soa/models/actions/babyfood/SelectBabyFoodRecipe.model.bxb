action (SearchBabyFoodList) {
  description ()
  collect {
    input (month) { //n개월
      type (Month)
      min (Optional) max (One)
    }
    input (ingredient) { //ex)재료: 고구마
      type (Ingredient)
      min (Optional) max (One)
    }
    input (babyFood) { //이유식, 아기이유식
      type (BabyFood)
      min (Required) max (One)
    }
    input (recommendation) { //추천해줘
      type (Recommendation)
      min (Optional) max (One)
    }
  }
  output (Results)
}
