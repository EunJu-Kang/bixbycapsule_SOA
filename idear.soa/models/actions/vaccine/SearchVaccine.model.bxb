action (SearchVaccine) {
  description (예방 접종 주사 정보)
  collect {
    input (vaccinationName) {
      type (VaccinationName)
      min (Required) max (One)
    }
  }
  type(Search)
  output (VaccinationInfo){
    on-empty{
      halt{
        dialog{
          template ("A형간염 알려줘 라고 말씀해주세요. 병명은 결핵,B형간염,디프테리아등이 있습니다.")
        }
      }
    }
  }
}
