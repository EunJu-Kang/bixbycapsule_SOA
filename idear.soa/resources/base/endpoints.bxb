endpoints {  
  // action과 그에 맞는 자바스크립트를 매핑
  action-endpoints {
    action-endpoint (GetHospitalInfo) {
      accepted-inputs (near, point, self)
      local-endpoint (hospital/GetHospitalInfo.js)
    }

     action-endpoint (HospitalMapApi) {
      accepted-inputs (point, self, hospitalConcept, search)
      local-endpoint (hospital/HospitalMapApi.js)
    }
  }
}
