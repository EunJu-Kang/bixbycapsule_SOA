result-view {
  match: BabyFoodList(this) 

  message {
    template ("아래의 이유식은 어떠세요?") {
    }
  }

  render {
      list-of (this){
        where-each (item){
          layout-macro (BabyFoodList){
            param(babyFood){
              expression (item)
            }
          }
        }
      }
  }
}