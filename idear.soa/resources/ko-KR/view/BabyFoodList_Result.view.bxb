result-view {
  match: BabyFoodList(this) 

  message {
    template ("아래의 이유식은 어떠세요?") {
    }
  }

  render {
    if(size(this) == 1){ 
        layout-macro (BabyFoodInfo) {
          param (info){ 
            expression (this) 
          }
        }
    } else{
      list-of (this){
        where-each (item){
          layout-macro (BabyFoodList){
            param(babyFood){
              expression (item)
            }
          }
        }
      }
    }
  }
}