layout-macro-def(VaccinViewLayout) { //메크로 이름
  params {
    param (resultData) { //넘겨줄 데아터 네임
      type (ResultsVaccin) //결괏값 출력하는 컨셉 이름
      min (Required)
      max (One)
    }
  }

  content {
    cell-card {
      slot2 {
        content {
            order (PrimarySecondary)
            primary ("#{value(resultData.vaccinInfo)}") 
            secondary ("자세한 정보를 보시려면 클릭하세요") 
        }
      }
    }
  }
}
