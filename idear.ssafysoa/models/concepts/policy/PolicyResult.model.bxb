structure (PolicyResult) {
  description (정책 결과를 위한 concept)
  features {transient}

  property (name) {
    type (PolicyName)
    min (Required) max (One)
  }
  property (content) {
    type (Content)
    min (Required) max (One)
  }
  property (target) {
    type (Target)
    min (Optional) max (Many)
  }
  property (benefit) {
    type (Benefit)
    min (Optional) max (Many)
  }
  property (tel) {
    type (Tel)
    min (Optional) max (Many)
  }
  property (site) {
    type (Site)
    min (Optional) max (Many)
  }
}
